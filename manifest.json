{
  "manifest_version": 3,
  "name": "Fck youtube ads",
  "version": "0.2",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "debugger"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/logo.png"
  }
}
